{
  "name": "@honeyroots/mobile",
  "version": "0.1002.0",
  "updateProductionVersion": "28",
  "updateStagingVersion": "23",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "cds": "node ./scripts/copy-design-system.mjs",
    "a": "./scripts/add_to_gitignore.sh",
    "r": "./scripts/remove_from_gitignore.sh",
    "start": "pnpm run cds && npx expo start -c --dev-client",
    "clean": "git clean -xdf .cache .expo .turbo android ios node_modules",
    "t-prod": "npx expo start --no-dev --minify",
    "prebuild": "npx expo prebuild --clean",
    "prebuild-a": "npx expo prebuild --clean -p android",
    "prebuild-i": "npx expo prebuild --clean -p ios",
    "local-dev-a-s": "pnpm run cds && pnpm run r && eas build --platform android --profile staging --local && pnpm run a",
    "local-dev-i-s": "pnpm run cds && pnpm run r && eas build --platform ios --non-interactive --profile staging --local && pnpm run a",
    "local-dev-i-p": "pnpm run cds  && pnpm run r && eas build --platform ios --non-interactive --profile production --local && pnpm run a",
    "local-dev-a-p": "pnpm run cds  && pnpm run r && eas build --platform android --profile  production --local && pnpm run a",
    "and": "npx expo run:android",
    "a-release": "npx expo run:android --variant release",
    "i-release": "npx expo run:ios --configuration Release",
    "dev": "npx expo start",
    "ios": "npx expo run:ios",
    "format": "prettier --check . --ignore-path ./gitignore",
    "lint": "npx eslint",
    "typecheck": "tsc --noEmit",
    "android": "expo run:android",
    "incrementProductionUpdateVersion": "node ./scripts/incrementProductionUpdateVersion.js",
    "incrementStagingUpdateVersion": "node ./scripts/incrementStagingUpdateVersion.js",
    "incrementMinorVersion": "node ./scripts/incrementMinorVersion",
    "device-i": "pnpm run cds && npx expo run:ios --device",
    "device-a": "pnpm run cds && npx expo run:android --device",
    "tunnel": "pnpm run cds && npx expo start -c --dev-client --tunnel",
    "update-s": "pnpm run cds && pnpm run incrementStagingUpdateVersion && eas update --branch staging && npx sentry-expo-upload-sourcemaps dist",
    "update-p": "pnpm run cds && pnpm run incrementProductionUpdateVersion && eas update --branch production && npx sentry-expo-upload-sourcemaps dist",
    "debug-a": "pnpm run cds && open -a \"/Applications/Android Studio.app\" ./android",
    "cloud-dev-i-s": "pnpm run cds && pnpm run r && eas build --platform ios --profile staging && pnpm run a",
    "cloud-dev-a-s": "pnpm run cds && pnpm run r && eas build --platform android --profile staging && pnpm run a",
    "cloud-dev-i-p": "pnpm run cds && pnpm run r && eas build --platform ios --profile production && pnpm run a",
    "cloud-dev-a-p": "pnpm run cds && pnpm run r && eas build --platform android --profile production && pnpm run a",
    "submit-all": "eas build --platform all --auto-submit --profile production --non-interactive",
    "deploy-i": "pnpm run cloud-prod-i-p && eas submit --platform ios --latest",
    "deploy-a": "pnpm run cloud-prod-a-p && eas submit --platform android --latest",
    "build": "./scripts/app-store-build-automation.sh",
    "create-d": "eas device:create",
    "check": "npx expo install --check",
    "patch": "patch-package"
  },
  "dependencies": {
    "@bacons/text-decoder": "^0.0.0",
    "@chatwoot/react-native-widget": "^0.0.21",
    "@clerk/clerk-expo": "^2.4.2",
