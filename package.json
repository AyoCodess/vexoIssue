    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.4"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-splash-screen"
      ]
    }
  },
  "prettier": "@honeyroots/prettier-config"
}
