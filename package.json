    "zustand": "5.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@honeyroots/eslint-config": "workspace:*",
    "@honeyroots/prettier-config": "workspace:*",
    "@honeyroots/tailwind-config": "workspace:*",
    "@honeyroots/tsconfig": "workspace:*",
